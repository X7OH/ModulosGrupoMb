	<queryString>
		<![CDATA[select p.value, p.upc, p.name as productname, pc.name as categoriaproducto, spc.name as categoria, sps.name as subcategoria,
       u.name as unidad, j.pricelist
from m_product p
left join m_transaction t on t.m_product_id = p.m_product_id
left join m_locator l on l.m_locator_id = t.m_locator_id
left join m_product_category pc on pc.m_product_category_id = p.m_product_category_id
left join slps_product_category spc on spc.slps_product_category_id = p.em_slps_product_category_id
left join slps_subcategory_prod sps on sps.slps_subcategory_prod_id = p.em_slps_subcategory_prod_id
left join c_uom u on u.c_uom_id = p.c_uom_id
left join m_productprice j ON j.m_product_id = p.m_product_id
LEFT JOIN m_pricelist_version k ON k.m_pricelist_version_id = j.m_pricelist_version_id
LEFT JOIN m_pricelist pl ON pl.m_pricelist_id = k.m_pricelist_id
where pl.isdefault = 'Y' AND pl.issopricelist = 'Y'
and p.value between (select value from m_product where m_product_id = $P{m_product_from_id}) and (select value from m_product where m_product_id = $P{m_product_to_id})
and (pc.m_product_category_id =  $P{m_product_category_id} or $P{m_product_category_id} is null)
and (spc.slps_product_category_id = $P{slps_product_category_id} or $P{slps_product_category_id}  is null)
and (sps.slps_subcategory_prod_id  =  $P{slps_subcategory_prod_id} or $P{slps_subcategory_prod_id}  is null)
group by 1,2,3,4,5,6,7,8
order by   pc.name, p.name,spc.name, sps.name]]>
	</queryString>
