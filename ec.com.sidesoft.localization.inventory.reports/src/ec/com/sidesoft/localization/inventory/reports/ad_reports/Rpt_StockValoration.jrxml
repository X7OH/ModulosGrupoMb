	<queryString>
		<![CDATA[SELECT M_PRODUCT_CATEGORY.NAME AS "categoriaproducto"
	, coalesce(spc.name,' ') as "categoria"
        ,coalesce(sps.name,' ') as "subcategoria"
,ZZ.M_PRODUCT_ID as "m_product_id"
,M_PRODUCT.UPC AS "upc"
,M_PRODUCT.VALUE AS "value"
,M_PRODUCT.NAME AS "name",
SUM(M.MOVEMENTQTY) AS stock, UOM_NAME as "unidad",
CASE ZZ.ISCOSTCALCULATED  WHEN 'Y'
THEN
(CASE WHEN SUM(TOTAL_COST)<>0 AND SUM(M.MOVEMENTQTY)<>0 THEN SUM(TOTAL_COST) / SUM(M.MOVEMENTQTY) ELSE 0 END)
ELSE 0  END AS "average_cost",
SUM(TOTAL_COST) AS "total_cost"
,M.M_LOCATOR_ID
,zz.bodegasub as "bodega"
FROM M_TRANSACTION M
left JOIN
	(
	SELECT M_PRODUCT.M_PRODUCT_CATEGORY_ID
	,spc.slps_product_category_id
	,sps.slps_subcategory_prod_id
	, A.M_PRODUCT_ID
	, C_UOM.NAME AS UOM_NAME
	, SUM(A.SUMA) AS TOTAL_COST
	, A.ISCOSTCALCULATED
	, A.AD_CLIENT_ID
	, A.C_CURRENCY_ID
	, A.M_TRANSACTION_ID
	,ML.VALUE as bodegasub
	,tr.m_locator_id
	FROM M_TRANSACTION TR
	LEFT JOIN
		( SELECT TRX.M_TRANSACTION_ID
			 , TRX.M_PRODUCT_ID
			 , CASE TRX.ISCOSTCALCULATED
			   WHEN 'Y'
				THEN C_CURRENCY_CONVERT_PRECISION (SUM(CASE WHEN TRX.MOVEMENTQTY < 0 THEN - TC.TRXCOST ELSE TC.TRXCOST END),TC.C_CURRENCY_ID,'100',TC.MOVEMENTDATE,NULL,TRX.AD_CLIENT_ID,TRX.ad_orG_id,'C')
			   ELSE 0 END AS SUMA
			 , TRX.C_UOM_ID
			 , TRX.AD_CLIENT_ID
			 , TRX.ISCOSTCALCULATED
			 , TC.C_CURRENCY_ID
			 FROM M_TRANSACTION TRX
			 JOIN M_LOCATOR L ON TRX.M_LOCATOR_ID = L.M_LOCATOR_ID
			 LEFT JOIN (	SELECT SUM(COST) AS TRXCOST
					, M_TRANSACTION_ID
					, C_CURRENCY_ID
					, COALESCE(DATEACCT, COSTDATE) as MOVEMENTDATE
					FROM M_TRANSACTION_COST
					WHERE COALESCE(DATEACCT, COSTDATE) <= $P{movementdate}
					GROUP BY m_transaction_id, C_CURRENCY_ID,
					COALESCE(DATEACCT, COSTDATE)) TC ON TRX.M_TRANSACTION_ID = TC.M_TRANSACTION_ID
			 WHERE TRX.MOVEMENTDATE <=   $P{movementdate}
			 GROUP BY TRX.M_TRANSACTION_ID, TRX.M_PRODUCT_ID, TRX.C_UOM_ID
			 , TRX.AD_CLIENT_ID, TRX.ISCOSTCALCULATED, TC.C_CURRENCY_ID
			 , TC.MOVEMENTDATE, TRX.AD_ORG_ID) A ON TR.M_TRANSACTION_ID = A.M_TRANSACTION_ID
	LEFT JOIN M_PRODUCT ON M_PRODUCT.M_PRODUCT_ID = M_PRODUCT.M_PRODUCT_ID
	left join slps_product_category spc on spc.slps_product_category_id = M_PRODUCT.em_slps_product_category_id
	left join slps_subcategory_prod sps on sps.slps_subcategory_prod_id = M_PRODUCT.em_slps_subcategory_prod_id
	LEFT JOIN M_LOCATOR ML ON ML.M_LOCATOR_ID = TR.M_LOCATOR_ID,C_UOM
	WHERE A.M_PRODUCT_ID = M_PRODUCT.M_PRODUCT_ID
	AND   A.C_UOM_ID = C_UOM.C_UOM_ID
	AND  ( A.SUMA <> 0 OR TR.MOVEMENTQTY <> 0)
	GROUP BY M_PRODUCT.M_PRODUCT_CATEGORY_ID, A.M_PRODUCT_ID, C_UOM.NAME
	, A.ISCOSTCALCULATED, A.AD_CLIENT_ID, A.C_CURRENCY_ID
	, A.M_TRANSACTION_ID,spc.slps_product_category_id,sps.slps_subcategory_prod_id,tr.m_locator_id,ML.VALUE
order by ML.VALUE

	) ZZ ON M.M_TRANSACTION_ID = ZZ.M_TRANSACTION_ID AND ZZ.M_LOCATOR_ID = M.M_LOCATOR_ID
LEFT JOIN M_PRODUCT ON M_PRODUCT.M_PRODUCT_ID =  ZZ.M_PRODUCT_ID
LEFT JOIN  M_PRODUCT_CATEGORY ON M_PRODUCT_CATEGORY.M_PRODUCT_CATEGORY_id = zz.M_PRODUCT_CATEGORY_id
left join slps_product_category spc on spc.slps_product_category_id = M_PRODUCT.em_slps_product_category_id
left join slps_subcategory_prod sps on sps.slps_subcategory_prod_id = M_PRODUCT.em_slps_subcategory_prod_id
left join m_locator ml on ml.m_locator_id = ZZ.m_locator_id
where (ml.m_locator_id = $P{m_locator_id} or $P{m_locator_id}is null)
and (m_product_category.m_product_category_id =  $P{m_product_category_id} or $P{m_product_category_id} is null)
and  (spc.slps_product_category_id = $P{slps_product_category_id}  or $P{slps_product_category_id} is null)
and (sps.slps_subcategory_prod_id =  $P{slps_subcategory_prod_id}  or  $P{slps_subcategory_prod_id}  is null)
and coalesce(total_cost,0)<>0
GROUP BY ZZ.M_PRODUCT_ID, M_PRODUCT_CATEGORY.NAME, UOM_NAME, ZZ.ISCOSTCALCULATED , spc.name,sps.name,M_PRODUCT.NAME ,zz.bodegasub
,M.M_LOCATOR_ID ,M_PRODUCT.UPC
,M_PRODUCT.VALUE
ORDER BY zz.bodegasub,
 M_PRODUCT_CATEGORY.NAME, spc.name,sps.name, M_PRODUCT.NAME]]>
	</queryString>
