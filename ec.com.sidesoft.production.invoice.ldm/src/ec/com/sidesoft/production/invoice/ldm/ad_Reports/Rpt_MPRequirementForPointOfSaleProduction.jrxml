	<queryString>
		<![CDATA[SELECT query.*,
(SELECT COALESCE(CB.NAME,AU.NAME) FROM AD_USER AU LEFT JOIN C_BPARTNER CB ON CB.C_BPARTNER_ID = AU.C_BPARTNER_ID WHERE AU.AD_USER_ID=$P{AD_USER_ID}) AS "usuario",
 NOW() AS "fechaimpresion"
 FROM (
SELECT LDM.*,((LDM.cant_stock-(LDM."bomqty"*LDM.cantidead_requerida))*-1) as "faltante" FROM(
SELECT INV.ad_org_id as "organizationid",
ORG.name as "localizacion",
INV.documentno as "documentno",
INV.dateinvoiced as "fecha_fac",
coalesce(PCTT.name,PCT.name) as "categoria",
PRL.value as "codigo",
PRL.name as "producto_requerido",
SUM((LIT.BOMQty*INVL.QtyInvoiced)) as "cantidead_requerida",
coalesce(UOMT.name,UOM.name) as "unidad",
STD.QtyOnHand as "cant_stock",
LIT.BOMQty as "bomqty"
--SUM((STD.QtyOnHand-(LIT.BOMQty*INVL.QtyInvoiced))*-1) as "faltante"
from c_invoice INV
inner join ad_org ORG on INV.ad_org_id = ORG.ad_org_id
inner join c_invoiceline INVL on INVL.c_invoice_id = INV.c_invoice_id
inner join m_product PRO on INVL.m_product_id = PRO.m_product_id
inner join M_Product_BOM LIT on LIT.m_product_id = PRO .m_product_id
inner join m_product PRL on LIT.M_ProductBOM_ID = PRL.m_product_id
inner join C_UOM UOM on PRL.C_UOM_ID = UOM.C_UOM_ID
inner join M_Storage_Detail STD on STD.m_product_id = PRL.m_product_id
inner join m_locator LOC on LOC.m_locator_id = STD.m_locator_id
inner join m_product_category PCT on PCT.m_product_category_id = PRL.m_product_category_id
left join m_product_category_trl PCTT on PCT.m_product_category_id = PCTT.m_product_category_id
left join c_uom_trl UOMT on UOM.C_UOM_ID = UoMT.C_UOM_ID
where (STD.QtyOnHand-(LIT.BOMQty*INVL.QtyInvoiced)) < 0
and LOC.m_locator_id = $P{M_LOCATOR_ID}
and INV.c_invoice_id= $P{C_INVOICE_ID}
and INV.ad_org_id= $P{ad_org_id}
group by "organizationid","localizacion","documentno","fecha_fac","categoria","codigo","producto_requerido","unidad","cant_stock","bomqty") LDM
UNION ALL
SELECT NLDM.*,((NLDM.cant_stock-NLDM.cantidead_requerida)*-1) as "faltante" FROM (select
INV.ad_org_id as "organizationid",
ORG.name as "localizacion",
INV.documentno as "documentno",
INV.dateinvoiced as "fecha_fac",
coalesce(PCTT.name,PCT.name) as "categoria",
PRO.value as "codigo",
PRO.name as "producto_requerido",
SUM(INVL.QtyInvoiced) as "cantidead_requerida",
coalesce(UOMT.name,UOM.name) as "unidad",
STD.QtyOnHand as "cant_stock",
to_number(0) as "bomqty"
--SUM((STD.QtyOnHand-INVL.QtyInvoiced)*-1) as "faltante"
from c_invoice INV
inner join ad_org ORG on INV.ad_org_id = ORG.ad_org_id
inner join c_invoiceline INVL on INVL.c_invoice_id = INV.c_invoice_id
inner join m_product PRO on INVL.m_product_id = PRO.m_product_id
inner join C_UOM UOM on PRO.C_UOM_ID = UOM.C_UOM_ID
inner join M_Storage_Detail STD on STD.m_product_id = PRO.m_product_id
inner join m_locator LOC on LOC.m_locator_id = STD.m_locator_id
inner join m_product_category PCT on PCT.m_product_category_id = PRO.m_product_category_id
left join m_product_category_trl PCTT on PCT.m_product_category_id = PCTT.m_product_category_id
left join c_uom_trl UOMT on UOM.C_UOM_ID = UoMT.C_UOM_ID
where (STD.QtyOnHand-INVL.QtyInvoiced) < 0
and PRO.isbom = 'N'
and LOC.m_locator_id = $P{M_LOCATOR_ID}
and INV.c_invoice_id= $P{C_INVOICE_ID}
and INV.ad_org_id= $P{ad_org_id}
group by "organizationid","localizacion","documentno","fecha_fac","categoria","codigo","producto_requerido","unidad","cant_stock","bomqty") NLDM) query
order by query.categoria DESC]]>
	</queryString>
