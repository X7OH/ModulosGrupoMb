	<queryString>
		<![CDATA[SELECT
ORG.name as "Organizacion",
PRC.name as "CategoryProduct",
PRO.value as "Codigo",
PRO.name as "Producto" ,
sum(INL.qtycount) as "cat_total",
sum(INL.qtybook) as "cant_teorica" ,
sum((INL.qtycount - INL.qtybook)) as "Diferencia",
sum(((INL.qtycount - INL.qtybook)*COS.cost)) as "valor_total",
(SELECT name FROM ad_user where ad_user_id =$P{ad_user} ) AS "usuario",
 NOW() AS "fechaimpresion",
UM.name as "Unidad",
(SELECT name from c_doctype where c_doctype_id = $P{c_doctype}) as "tipo_doc"
FROM m_inventory INV
inner join m_inventoryline INL on INL.m_inventory_id = INV.m_inventory_id
inner join m_product PRO on PRO.m_product_id = INL.m_product_id
inner join M_Product_Category PRC on PRO.M_Product_Category_ID = PRC.M_Product_Category_ID
inner join ad_org ORG on ORG.ad_org_id = INL.ad_org_id
inner join m_costing COS on COS.m_product_id = PRO.m_product_id
inner join c_uom UM on UM.c_uom_id = PRO.c_uom_id
WHERE (INL.ad_org_id = $P{ad_org} or $P{ad_org} is null) and
(INL.m_locator_id = $P{m_warehause} or $P{m_warehause} is null) and
(INV.movementdate >=  $P{date_start} and INV.movementdate <=  $P{date_end}) and
(INV.em_ssin_doctype_id = $P{c_doctype} or $P{c_doctype} is null ) and
(PRO.M_Product_Category_ID = $P{p_category} or $P{p_category} is null) and
(COS.datefrom = (SELECT MAX(COS.datefrom) from m_costing COS where COS.m_product_id = PRO.m_product_id  and datefrom <= to_date(INV.movementdate,'DD-MM-YYYY')+1)) and
(COS.created = (SELECT MAX(COS.created) from m_costing COS where COS.m_product_id = PRO.m_product_id  and datefrom <=  to_date(INV.movementdate,'DD-MM-YYYY')+1))
group by
ORG.name,
PRC.name,
PRO.value,
PRO.name,
UM.name
order by ORG.name, PRC.name, PRO.name]]>
	</queryString>
