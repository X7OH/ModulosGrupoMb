FROM sccc_cash_clousure cc
LEFT JOIN ad_user userc ON userc.ad_user_id = cc.createdby
LEFT JOIN ad_user useru ON useru.ad_user_id = cc.updatedby
LEFT JOIN ad_org org On org.ad_org_id = cc.ad_org_id
WHERE cc.sccc_cash_clousure_id = $P{sccc_cash_clousure_id}]]>
