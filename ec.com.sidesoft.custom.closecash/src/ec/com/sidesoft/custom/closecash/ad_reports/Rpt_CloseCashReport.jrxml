		<![CDATA[SELECT 	org.ad_org_id as "ad_org_id",
	org.name as "organization",
	cc.description as "nombrecierre",
	userc.name as "creadopor",
	to_char(cc.created,'dd') || ' de ' || to_char(cc.created,'TMMonth') || ' del '||to_char(cc.created,'yyyy HH24:MI:SS') as "creacionfecha",
	useru.name as "actualizadopor",
