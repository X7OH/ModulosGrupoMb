	<queryString>
		<![CDATA[SELECT COALESCE((SELECT SUM(co.totalLines) FROM c_order co WHERE co.issotrx = 'Y'AND co.docstatus = 'CO'
	AND to_char( co.dateordered ,'yyyy-MM-dd') = to_char( cc.closingdate ,'yyyy-MM-dd')
	AND co.c_doctypetarget_id = postype.c_doctype_id),0) as "ventasnetas",
	COALESCE((SELECT SUM(cotax.taxamt) FROM c_order co
		LEFT JOIN c_ordertax cotax ON cotax.c_order_id = co.c_order_id
		LEFT JOIN c_tax tax ON tax.c_tax_id = cotax.c_tax_id
		WHERE co.issotrx = 'Y'AND co.docstatus = 'CO'
		AND to_char( co.dateordered ,'yyyy-MM-dd') = to_char( cc.closingdate ,'yyyy-MM-dd')
		AND co.c_doctypetarget_id = postype.c_doctype_id AND tax.rate = 0),0)as "iva0",
	COALESCE((SELECT SUM(cotax.taxamt) FROM c_order co
		LEFT JOIN c_ordertax cotax ON cotax.c_order_id = co.c_order_id
		LEFT JOIN c_tax tax ON tax.c_tax_id = cotax.c_tax_id
		WHERE co.issotrx = 'Y'AND co.docstatus = 'CO'
		AND to_char( co.dateordered ,'yyyy-MM-dd') = to_char( cc.closingdate ,'yyyy-MM-dd')
		AND co.c_doctypetarget_id = postype.c_doctype_id AND tax.rate = 12),0)as "iva12",
	COALESCE((SELECT SUM(co.totalLines) FROM c_order co WHERE co.issotrx = 'Y'
		AND to_char( co.dateordered ,'yyyy-MM-dd') = to_char( cc.closingdate ,'yyyy-MM-dd')
		AND co.c_doctypetarget_id = postype.c_doctyperet_id),0) as "devonetas",
	COALESCE((SELECT SUM(co.grandtotal) FROM c_order co WHERE co.issotrx = 'Y'
		AND to_char( co.dateordered ,'yyyy-MM-dd') = to_char( cc.closingdate ,'yyyy-MM-dd')
		AND co.c_doctypetarget_id = postype.c_doctyperet_id),0) as "devobrutas"
FROM sccc_cash_clousure cc
LEFT JOIN ad_org org On org.ad_org_id = cc.ad_org_id
LEFT JOIN sccc_setup setup ON setup.ad_org_id = org.ad_org_id
LEFT JOIN obpos_applications pos ON pos.obpos_applications_id = setup.em_scccd_posterminal
LEFT JOIN obpos_terminaltype postype ON postype.obpos_terminaltype_id = pos.obpos_terminaltype_id
WHERE cc.sccc_cash_clousure_id = $P{sccc_cash_clousure_id}]]>
	</queryString>
