	<subDataset name="Table Dataset 1">
		<parameter name="AD_ORG_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="F_DESDE" class="java.util.Date">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="F_HASTA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select monthforecast,weekforecast,count(m_product_id),m_product_id,ao.name
from scmf_dateforecast df
left join m_transaction mt  on to_char(movementdate,'dd/MM/yyyy') = df.stringdate
left join ad_org ao on mt.ad_org_id = ao.ad_org_id
where
mt.movementdate between  $P{F_DESDE} and  $P{F_HASTA}
AND (AO.AD_ORG_ID = $P{AD_ORG_ID} OR $P{AD_ORG_ID} IS NULL)
group by
weekforecast,m_product_id,monthforecast,ao.name
--order by 1,2,3,5
order by weekforecast,m_product_id]]>
		</queryString>
		<field name="monthforecast" class="java.math.BigDecimal"/>
		<field name="weekforecast" class="java.math.BigDecimal"/>
		<field name="count" class="java.lang.Long"/>
		<field name="m_product_id" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
	</subDataset>
	<queryString>
		<![CDATA[select monthforecast,weekforecast,count(m_product_id),m_product_id,ao.name
from scmf_dateforecast df
left join m_transaction mt  on to_char(movementdate,'dd/MM/yyyy') = df.stringdate
left join ad_org ao on mt.ad_org_id = ao.ad_org_id
where
mt.movementdate between '2015-09-01' and '2016-12-31'
and ao.name = 'FARMACIA COTOCOLLAO'
and m_product_id = '0016B4F89D0248E3A4CC08D201F20175'
group by
weekforecast,m_product_id,monthforecast,ao.name
--order by 1,2,3,5
order by weekforecast,m_product_id]]>
	</queryString>
