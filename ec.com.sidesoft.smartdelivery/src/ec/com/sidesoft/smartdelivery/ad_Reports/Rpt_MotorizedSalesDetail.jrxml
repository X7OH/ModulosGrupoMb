	<parameter name="EndTime" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 	ORG.ad_org_id as "ad_org_id",
	ORG.name as "organizacion",
	MOT.name as "empleado",
	MOT.taxid as "cedula",
	COR.documentno as "numeroDocumento",
	to_char(COR.dateordered, 'dd/MM/YYYY') as "fechapedido",
	CBP.name as "tercero",
	COALESCE(COR.grandtotal, 0) as "importetotal",
	FPM.name as "metodo de pago",
	to_char(now(),'dd-MM-yyyy') as "fecha",
	to_char(now(),'HH24:MI:SS') as "hora"
FROM c_order COR
INNER JOIN ad_org ORG on ORG.ad_org_id = COR.ad_org_id
LEFT JOIN c_bpartner CBP ON CBP.c_bpartner_id = COR.c_bpartner_id
LEFT JOIN c_bpartner MOT ON MOT.c_bpartner_id = COR.em_ssmrdr_motorized_id
LEFT JOIN fin_paymentmethod FPM ON FPM.fin_paymentmethod_id = COR.fin_paymentmethod_id
WHERE COR.issotrx = 'Y'
AND to_date(to_char(COR.created,'dd-MM-YYYY'),'dd/MM/YYYY') >= $P{DateFrom}
AND to_char(COR.created,'HH24:MI:SS') >= $P{StartTime}
AND to_date(to_char(COR.created,'dd-MM-YYYY'),'dd/MM/YYYY') <= $P{DateTo}
AND to_char(COR.created,'HH24:MI:SS') <= $P{EndTime}
AND COR.em_ssmrdr_motorized_id IS NOT NULL
AND COR.em_sscmb_sales_origin IN ('WEB','CLC')
AND (COR.ad_org_id = $P{AD_Org_ID} or $P{AD_Org_ID} is null)
AND (MOT.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null)
ORDER BY MOT.taxid]]>
	</queryString>
