	<parameter name="StartDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="MA_Processplan_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	documentno  as "documentno"
	,MAPP.name as "producto"
	,MAPP.Secondaryunit as "unidad"
	,MAP.Quantity as "cant_planificada"
	,MAP.Donequantity as "cat_producida"
	,to_char(round((MAP.Donequantity*100/
	(CASE
		WHEN coalesce(MAP.Quantity,1)<>0 THEN
			coalesce(MAP.Quantity,1)
		ELSE 1
	END)),2))
	as "cumplimiento"
,(CASE
		WHEN MAWR.EndDate>to_date($P{EndDate},'YYYY-MM-DD')THEN
			to_char('Y')
		ELSE
		to_char('N')
	END) as "desfasado"

,round((MAP.Donequantity*100/
	(CASE
		WHEN coalesce(COUNTER.cat_producida,1)<>0 THEN
			coalesce(COUNTER.cat_producida,1)
		ELSE 1
	END)),2) as "total_cat_produc"
,MAPP.ad_org_id as "organization"
 FROM (SELECT
					SUM(MAPc.Donequantity) as "cat_producida"
				FROM MA_WorkRequirement MAWRc
					INNER JOIN MA_Processplan MAPPc on
						MAPPc.MA_Processplan_ID = MAWRc.MA_Processplan_ID
					INNER JOIN MA_WRPhase MAPc on
						MAPc.MA_WorkRequirement_id = MAWRc.MA_WorkRequirement_id
WHERE MAPPc.MA_Processplan_ID = $P{MA_Processplan_ID}
AND  MAWRc.StartDate BETWEEN $P{StartDate} and $P{EndDate}
				) COUNTER ,MA_WorkRequirement MAWR
	INNER JOIN MA_Processplan MAPP on
					MAPP.MA_Processplan_ID = MAWR.MA_Processplan_ID
	INNER JOIN MA_WRPhase MAP on MAP.MA_WorkRequirement_id = MAWR.MA_WorkRequirement_id
WHERE MAPP.MA_Processplan_ID = $P{MA_Processplan_ID}
AND MAWR.StartDate BETWEEN $P{StartDate} and $P{EndDate}
order by documentno]]>
	</queryString>
