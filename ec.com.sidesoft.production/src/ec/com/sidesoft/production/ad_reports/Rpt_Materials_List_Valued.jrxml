	<parameter name="Ad_Org_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Ad_User_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="M_Product_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="M_Product_Category_Id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
CASE WHEN  $P{Ad_Org_Id} IS NULL THEN
(select ao.name from ad_org ao where ao.ad_orgtype_id='1')
ELSE
(select ao.name from ad_org ao where ao.ad_org_id = $P{Ad_Org_Id})
END AS "organizacion",
(select ao.ad_org_id from ad_org ao where ao.ad_orgtype_id='1') as "ad_org_id",
pc.name AS "categoria",
mp.value AS "codigoldm",
mp.m_product_id,
mp.name AS "nombreldm",
coalesce(uot.name,uo.name) AS "unidadldm",
prb.line AS "line",
mp2.value AS "codigo",
mp2.name AS "nombre",
coalesce(uot2.name,uo2.name) AS "unidad",
coalesce(prb.BOMQty,0) AS "cantidad",
coalesce(mc.cost,0) AS "costounitario",
round(coalesce(prb.BOMQty,0)*coalesce(mc.cost,0),5) AS "costototal",
(select
coalesce(cbu.name,adu.name) as usuario
from ad_user adu
left join c_bpartner cbu on cbu.c_bpartner_id = adu.c_bpartner_id
where adu.ad_user_id = $P{Ad_User_Id}) as "usuario",
now() as "fechaimpresion"
FROM m_product mp
LEFT JOIN m_product_category pc ON mp.m_product_category_id = pc.m_product_category_id
LEFT JOIN c_uom uo ON mp.c_uom_id = uo.c_uom_id
LEFT JOIN c_uom_trl uot ON mp.c_uom_id = uot.c_uom_id AND upper(uot.ad_language)='ES_ES'
INNER JOIN M_Product_BOM prb ON mp.m_product_id = prb.m_product_id
INNER JOIN m_product mp2 ON prb.M_ProductBOM_ID = mp2.m_product_id
LEFT JOIN c_uom uo2 ON mp2.c_uom_id = uo2.c_uom_id
LEFT JOIN c_uom_trl uot2 ON mp2.c_uom_id = uot2.c_uom_id AND upper(uot2.ad_language)='ES_ES'
LEFT JOIN M_Costing mc ON mp2.m_product_id = mc.m_product_id and mc.dateto=
(SELECT max(mc2.dateto) FROM m_costing mc2 WHERE  mc2.m_product_id = mp2.m_product_id)
WHERE mp.IsBOM ='Y' AND
(mp.ad_org_id = $P{Ad_Org_Id} OR $P{Ad_Org_Id} IS NULL OR $P{Ad_Org_Id}='0') AND
(pc.m_product_category_id = $P{M_Product_Category_Id} OR $P{M_Product_Category_Id} IS NULL) AND
(mp.m_product_id = $P{M_Product_Id} OR $P{M_Product_Id} IS NULL)
order by pc.name,mp.name,prb.line]]>
	</queryString>
