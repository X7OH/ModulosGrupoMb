	<queryString>
		<![CDATA[SELECT  CBP.TaxID as "ruc",
CBP.name as "proveedor",
INV.poreference as "factura",
INV.GrandTotal as "total_imprt",
INV.Totalpaid as "total_pagado",
INV.OutstandingAmt as "total_pendiente"
FROM c_invoice INV
INNER JOIN c_bpartner CBP ON CBP.c_bpartner_id = INV.c_bpartner_id
WHERE issotrx  ='N'
AND em_sscmb_wait_cn = 'Y'
AND (INV.c_bpartner_id = $P{C_BPartner_ID} or $P{C_BPartner_ID} is null)
AND dateacct between $P{STARTDATE} and (to_date($P{ENDDATE},'yyyy-mm-dd')+1)]]>
	</queryString>
