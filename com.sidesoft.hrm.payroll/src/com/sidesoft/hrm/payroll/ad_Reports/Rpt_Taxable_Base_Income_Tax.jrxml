	<queryString>
		<![CDATA[SELECT mi_query.*
	,mi_query.total_ingresos-mi_query.total_gastos-mi_query.total_exoneraciones as  "base_imponible"
	,(SELECT COALESCE(CB.NAME,AU.NAME) FROM AD_USER AU LEFT JOIN C_BPARTNER CB ON CB.C_BPARTNER_ID = AU.C_BPARTNER_ID WHERE AU.AD_USER_ID=$P{AD_USER_ID}) AS "usuario"
	,NOW() 	AS "fechaimpresion"

FROM (
SELECT PYR.ad_org_id,BPA.em_sspr_documentno as "cedula"
	,BPA.name 	as "nombre_completo"
	,POS.name as "cargo"
	,PYRTC.ingr_relacion_dep as "ingr_relacion_dep"
	,COALESCE(IOE.ingr_otros_empleadores,0) as "ingr_otros_empleadores"
	,(PYRTC.ingr_relacion_dep+COALESCE(IOE.ingr_otros_empleadores,0)) as "total_ingresos"
	,IOE.aporte_iess_otros_e as "aporte_iess_otros_e"
	,PYRTC.aporte_iess_este_e as "aporte_iess_este_e"
	,SSCSTE.alimentos/12 as "alimentos"
	,SSCSTE.salud/12 as "salud"
	,SSCSTE.vestimenta/12 as "vestimenta"
	,SSCSTE.educacion/12 as "educacion"
	,SSCSTE.vivienda/12 as "vivienda"
	,(COALESCE(PYRTC.aporte_iess_este_e,0)
		+COALESCE(IOE.aporte_iess_otros_e,0)
		+COALESCE(SSCSTE.vivienda/12,0)
		+COALESCE(SSCSTE.salud/12,0)
		+COALESCE(SSCSTE.educacion/12,0)
		+COALESCE(SSCSTE.alimentos/12,0)
		+COALESCE(SSCSTE.vestimenta/12,0)) as "total_gastos"
	,CASE WHEN SDSB.percentage is not null THEN
		((SITAX.basemax_min*2)*(SDSB.percentage/100))/12
	ELSE 0 END as "exoneracion_discapacidad"
	,CASE WHEN age(NOW(),BPA.em_sspr_birthday)>interval '65 year' THEN
		SITAX.basemax_min/12
		ELSE
		0
	END as "exoneracion_tercera_e"
	,(CASE WHEN SDSB.percentage is not null THEN
		((SITAX.basemax_min*2)*(SDSB.percentage/100))/12
	ELSE 0 END)+(CASE WHEN age(NOW(),BPA.em_sspr_birthday)>interval '65 year' THEN
		SITAX.basemax_min/12
		ELSE
		0
	END) as "total_exoneraciones"
	,PYRTC.impuesto_renta as "impuesto_renta"
	,sescr_monthtoletters(to_number(TO_CHAR(PER.startdate,'MM')))||' / '||YAR.year as "periodo"
FROM sspr_payroll  PYR
	JOIN c_period PER on PER.c_period_id = PYR.c_period_id
	JOIN c_year YAR on YAR.c_year_id = PER.c_year_id
	JOIN sspr_payroll_ticket PYRT on PYRT.sspr_payroll_id = PYR.sspr_payroll_id
	JOIN c_bpartner BPA on BPA.c_bpartner_id = PYRT.c_bpartner_id
	JOIN sspr_contract CTR on CTR.c_bpartner_id = BPA.c_bpartner_id
				AND ((PER.startdate BETWEEN CTR.startdate and CTR.enddate
						OR PER.enddate BETWEEN CTR.startdate and CTR.enddate)
					AND CTR.startdate = (SELECT MAX(SCTR.startdate)
								FROM sspr_contract SCTR
								WHERE SCTR.c_bpartner_id = BPA.c_bpartner_id
									AND (PER.startdate BETWEEN SCTR.startdate and SCTR.enddate
										OR PER.enddate BETWEEN SCTR.startdate and SCTR.enddate
									)
							)
					)
	LEFT JOIN sspr_contract_position CTRP on CTRP.isactive='Y' AND CTRP.sspr_contract_id = CTR.sspr_contract_id
	LEFT JOIN sspr_position POS on POS.sspr_position_id = CTRP.sspr_position_id

	LEFT JOIN (
			SELECT SUB.sspr_payroll_ticket_id
				,SUM(SUB.ingr_relacion_dep) as "ingr_relacion_dep"
				,SUM(SUB.aporte_iess_este_e) as "aporte_iess_este_e"
				,SUM(SUB.impuesto_renta) as "impuesto_renta"

			FROM 	(SELECT
					CASE WHEN (CNP.conceptsubtype = 'In' AND (CNP.isincomecalculated= 'Y' OR CNP.iscumulative='Y' OR CNP.isprojected='Y' OR CNP.isiess = 'Y')) THEN
						PTC.amount
						ELSE 0
					END as "ingr_relacion_dep"
					,CASE WHEN FMY.typeconcept = 'PCITE' THEN --Aporte Personal IESS con Este Empleador
						PTC.amount
						ELSE 0
					END as "aporte_iess_este_e"
					,CASE WHEN FMY.typeconcept = 'VTWWTE' THEN -- Valor del Impuesto Asumido por Este Empleado
						PTC.amount
						ELSE 0
					END as "impuesto_renta"

					,PTC.sspr_payroll_ticket_id
				FROM sspr_payroll_ticket_concept PTC
					JOIN sspr_concept CNP on CNP.sspr_concept_id = PTC.sspr_concept_id
					LEFT JOIN sspr_codeformulary107 FMY on FMY.sspr_codeformulary107_id = CNP.sspr_codeformulary107_id
				) SUB
			GROUP BY SUB.sspr_payroll_ticket_id
		)  PYRTC on PYRTC.sspr_payroll_ticket_id = PYRT.sspr_payroll_ticket_id
LEFT JOIN (SELECT SUM(SFMY.ingr_otros_empleadores)/12 as "ingr_otros_empleadores"
				,SUM(SFMY.aporte_iess_otros_e)/12 as "aporte_iess_otros_e"
				,SFMY.c_year_id
				,SFMY.c_bpartner_id
			FROM
			(SELECT
				CASE WHEN CFMY.typeconcept='TIOE' THEN --Ingresos Gravados Generados con Otros Empleadores
					FMYL.amount
					ELSE 0
				END as "ingr_otros_empleadores"
				,CASE WHEN CFMY.typeconcept = 'PCIOE' THEN --Aporte Personal IESS con Otros Empleadores
					FMYL.amount
					ELSE 0
				END as "aporte_iess_otros_e"
				,FMY.c_year_id
				,FMYL.c_bpartner_id
			FROM sspr_formulary107 FMY
				LEFT JOIN sspr_formularyline107 FMYL on FMYL.sspr_formulary107_id = FMY.sspr_formulary107_id
				LEFT JOIN sspr_codeformulary107 CFMY on CFMY.sspr_codeformulary107_id = FMYL.sspr_codeformulary107_id
			) SFMY
			GROUP BY SFMY.c_year_id,SFMY.c_bpartner_id) IOE
		ON IOE.c_year_id=PER.c_year_id AND IOE.c_bpartner_id = BPA.c_bpartner_id
	LEFT JOIN (
			SELECT
				SUM(SCSTE.alimentos) as "alimentos"
				,SUM(SCSTE.salud) as "salud"
				,SUM(SCSTE.vestimenta) as "vestimenta"
				,SUM(SCSTE.educacion) as "educacion"
				,SUM(SCSTE.vivienda) as "vivienda"
				,SCSTE.c_year_id
				,SCSTE.c_bpartner_id
			FROM (
				SELECT CSTE.c_year_id ,CSTE.c_bpartner_id
					,CASE WHEN CSTEL.deductibleexpense = 'F' THEN-- ALIMENTOS
						CSTEL.amountdeductible
						ELSE 0 END as "alimentos"
					,CASE WHEN CSTEL.deductibleexpense = 'HE' THEN-- SALUD
						CSTEL.amountdeductible
						ELSE 0 END as "salud"
					,CASE WHEN CSTEL.deductibleexpense = 'C' THEN-- Vestimenta
						CSTEL.amountdeductible
						ELSE 0 END as "vestimenta"
					,CASE WHEN CSTEL.deductibleexpense = 'E' THEN-- Educacion
						CSTEL.amountdeductible
						ELSE 0 END as "educacion"
					,CASE WHEN CSTEL.deductibleexpense = 'H' THEN-- Vivienda
						CSTEL.amountdeductible
						ELSE 0 END as "vivienda"
				FROM sspr_costemployee CSTE
				INNER JOIN sspr_costemployeeline CSTEL on CSTE.sspr_costemployee_id = CSTEL.sspr_costemployee_id AND CSTEL.isactive='Y'
			) SCSTE
			GROUP BY SCSTE.c_year_id,SCSTE.c_bpartner_id
		) SSCSTE on SSCSTE.c_year_id=PER.c_year_id AND SSCSTE.c_bpartner_id= BPA.c_bpartner_id
	LEFT JOIN (SELECT ITAX.sspr_incometax_id,ITAX.c_year_id,MIN(ITAXL.basemax) as "basemax_min"
		FROM sspr_incometax ITAX
			JOIN sspr_incometaxline ITAXL  on ITAXL.sspr_incometax_id =ITAX.sspr_incometax_id
		GROUP BY ITAX.sspr_incometax_id,ITAX.c_year_id) SITAX on SITAX.c_year_id = PER.c_year_id
	LEFT JOIN (SELECT c_year_id,grade_disability_from,grade_disability_to ,percentage
			FROM sspr_disability DSB
			JOIN sspr_disabilityline DSBL on DSBL.sspr_disability_id = DSB.sspr_disability_id
		) SDSB on SDSB.c_year_id = PER.c_year_id
					AND to_number(BPA.em_sspr_disability) BETWEEN SDSB.grade_disability_from AND SDSB.grade_disability_to
		WHERE ispayroll = 'Y'
	AND PYR.documentno = $P{DOCUMENT_NO}

) mi_query
ORDER BY  mi_query.nombre_completo]]>
	</queryString>
