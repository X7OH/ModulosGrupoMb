	<queryString>
		<![CDATA[select
anio_entrega
,mes_entrega
,dia_entrega
,taxid_org
,org
,year
,c_year_id
,a.c_bpartner_id
,taxid_employee
,empleado
,tipoidentificacion
,numeroidentificacion
,empleadoapellido
,empleadonombre
,codigoestab
,pais
,codigopais
,aplicaconvenio
,discapacitado
,porcentajediscapacidad
,tipoidentdiscapacidad
,numeroidentifdiscapacidad
,sueldo
,bonos
,utilidades + coalesce(util.utilidad,0) as utilidades
,impuestorenta
,decimotercero
,decimocuarto
,fondosreserva
,compensacionsalariodigno
,otrosing_rentagravada
,ingresosgrav349
,sistemasalariodigno
,aportepersonal
,impuestorentacausado
,valorimprettrabajador
,gastosvivienda
,gastossalud
,gastoseducacion
,gastoalimentacion
,gastosvestimenta
,ingresosgravados
,aportepersonalo
,exoneracionpordiscapacidad
,case when exoneracionpordiscapacidad <> 0 then 0 else exoneracionportercerasedad end as exoneracionportercerasedad
,valorimpret403
,valorimpasumido405
,baseimponiblegrav
,documentno
,ruccontador
, replace($P{BASE_DESIGN},'src-loc/design','web') as "logo"
,gastosturismo
,rebajagastospersonales
,despuesrebajagastospersonales
from sspr_formulary107_detail_v a
LEFT JOIN (
SELECT
SU.C_BPARTNER_ID
,CBP.TAXID
,SU.TOTALPROFITS AS UTILIDAD
,SU.sspr_codeformulary107_id
,c_y.YEAR as anio
FROM sspr_utilities SU
LEFT JOIN C_YEAR C_Y ON C_Y.C_YEAR_ID = SU.C_YEAR_ID
LEFT JOIN C_BPARTNER CBP ON CBP.C_BPARTNER_ID = SU.C_BPARTNER_ID
WHERE TO_NUMBER(trim(c_y.YEAR)) = (SELECT TO_NUMBER(CY.YEAR)-1  AS ANIO FROM C_YEAR CY WHERE CY.C_YEAR_ID =$P{C_YEAR_ID} )
and su.sspr_codeformulary107_id in ( select sspr_codeformulary107_id from sspr_codeformulary107  where typeconcept = 'UT')
) util on util.taxid = a.taxid_employee
where a.c_year_id = $P{C_YEAR_ID}
and (a.c_bpartner_id = $P{C_BPARTNER_ID} or $P{C_BPARTNER_ID} is null)]]>
	</queryString>
